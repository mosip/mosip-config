<routes xmlns="http://camel.apache.org/schema/spring">
   <!-- printing-bus-in to credential-requestor-bus-in Route is kept for backward compatibility. If fresh deployment is done, this route can be removed -->
   <route id="printing-bus-in route-->credential-requestor-bus-in route">
      <from uri="eventbus://printing-bus-in" />
      <log message="printing-bus-in--> credential-requestor-bus-in route ${bodyAs(String)}" />
      <to uri="eventbus://credential-requestor-bus-in" />
   </route>
</routes>
