                <to uri="eventbus:uin-generator-bus-in" />
            </when>
            <when>
                <simple>${bodyAs(String)} contains '"address":"abis-handler-bus-in"'</simple>
                <to uri="eventbus:abis-handler-bus-in" />
            </when>
            <when>
