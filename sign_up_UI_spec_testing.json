            "fieldType": "default",
            "format": "none",
            "validators": [
                {
                    "regex": "^(1869|18[7-9][0-9]|19[0-9][0-9]|20[0-9][0-9])-([0][1-9]|1[0-2])-([0][1-9]|[1-2][0-9]|3[01])$",
                    "error": {
                        "eng": "Date must be in DD-MM-YYYY format between 1869 and 2099",
                        "khm": "កាលបរិច្ឆេទត្រូវតែមានទម្រង់ DD-MM-YYYY ចន្លោះពីឆ្នាំ 1869 ដល់ 2099",
                        "hin": "Date must be in DD-MM-YYYY format between 1869 and 2099",
                        "kan": "Date must be in DD-MM-YYYY format between 1869 and 2099",
                        "tam": "Date must be in DD-MM-YYYY format between 1869 and 2099",
                        "ara": "Date must be in DD-MM-YYYY format between 1869 and 2099"
                    },
                    "langCode": "en"
                }
            ],
            "required": false
        },
