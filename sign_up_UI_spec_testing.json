        {
            "id": "fullName",
            "capsLockCheck": true,
            "controlType": "textbox",
            "type": "simpleType",
            "labelName": {
                "en": "Full Name in Khmer",
                "km": "គោត្តនាម-នាម"
            },
            "placeholder": {
                "en": "Enter Full Name in Khmer",
                "km": "បញ្ចូលគោត្តនាម-នាមជាភាសាខ្មែរ"
            },
            "validators": [
                {
                    "regex": "^[\u1780-\u17FF\u19E0-\u19FF\u1A00-\u1A9F\u0020]{1,30}$",
                    "error": {
                        "en": "Full Name has to be in Khmer only",
                        "km": "គោត្តនាម-នាមត្រូវតែមានតែអក្សរខ្មែរ"
                    },
                    "langCode": "km"
                },
                {
                    "regex": "^[a-zA-Z][a-zA-Z ]{1,30}$",
                    "error": {
                        "en": "Full Name has to be in English only",
                        "km": "ឈ្មោះ​ពេញ​ត្រូវតែសរសេរជាភាសាអង់គ្លេសតែប៉ុណ្ណោះ"
                    },
                    "langCode": "en"
                }
            ],
            "info": {
                "en": "Maximum 30 characters allowed.",
                "km": "អនុញ្ញាតអតិបរមា 30 តួអក្សរ។"
            },
            "alignmentGroup": "groupB",
            "required": true
        },
        {
            "id": "password",
            "capsLockCheck": true,
            "controlType": "password",
            "labelName": {
                "en": "Password",
                "km": "ពាក្យសម្ងាត់"
            },
            "placeholder": {
                "en": "Enter your password",
                "km": "សូមបញ្ចូលពាក្យសម្ងាត់"
            },
            "validators": [
                {
                    "regex": "^(?=.*[0-9])(?=.*[a-z])(?=.*[A-Z])(?=.*[\\x5F\\W])(?=.{8,20})[a-zA-Z0-9\\x5F\\W]{8,20}$",
                    "error": {
                        "en": "Password does not meet the policy",
                        "km": "ពាក្យសម្ងាត់មិនបំពេញតាមគោលការណ៍"
                    }
                }
            ],
            "alignmentGroup": "groupC",
