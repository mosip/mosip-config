            "capsLockCheck": true,
            "controlType": "textbox",
            "type": "simpleType",
            "label": {
                "eng": "Name in Khmer",
                "khm": "គោត្តនាម-នាម",
                "hin": "पूरा नाम",
                "kan": "ಪೂರ್ಣ ಹೆಸರು",
                "tam": "முழு பெயர்",
                "ara": "الاسم الكامل"
            },
            "placeholder": {
                "en": "Full Name in Khmer",
                "km": "បញ្ចូលគោត្តនាម-នាមជាភាសាខ្មែរ",
                "hin": "पूरा नाम",
                "kan": "ಪೂರ್ಣ ಹೆಸರು",
                "tam": "முழு பெயர்",
                "ara": "الاسم الكامل"
